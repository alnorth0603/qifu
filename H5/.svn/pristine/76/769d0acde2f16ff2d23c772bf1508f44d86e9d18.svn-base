.s-homeBG{
    position: fixed;
    top: 0;
    left: 0;
    bottom: 0;
    width: 100%;
    height: 100%;
    z-index: 1;
}
.s-middelDiv{
    position: relative;
    z-index: 2;
    width: 60%;
    margin: 115px auto 0;
}
.s-middelDiv img{
	width: 100%;
}
.s-middelDiv .s-title{
	text-align: center;
    margin-top: 2rem;
}
.s-middelDiv .s-title a{
    color: #BFC3C6;
    font-size: 22px;
}

.s-whMenu{
    position: absolute;
    bottom: 5%;
    z-index: 2;
    width: 100%;
}
.s-whMenu .wheel-button{
    width: 30%;
    margin: auto;
}
.s-whMenu ul{
    margin: 0;
    padding: 0;
    width: 100%;
    text-align: center;
}
.s-whMenu ul li{
    width: 18%;
    display: inline-block;
    position: absolute;
    top: -20%;
}
.s-whMenu .s-bottomDiv{
    text-align: center;
    font-size: 14px;
    font-weight: bold;
}

