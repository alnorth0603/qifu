.page .bottomBtn{
	position: fixed;
	bottom: 0;
	background-color: #FFFFFF;
	text-align: center;
	width: 100%;
    height: 2.5rem;
    line-height: 2.5rem;
}
.page .content .list-block .item-content{
	padding: 0;
}
.page .content .list-block .item-link .item-inner{
    padding-left: .5rem;
    min-height: 2.5rem;
}
.page .content .list-block .item-link.last .item-inner{
	background-image: none;
}
/*我的设置*/
#mySetting-page{
	background-color: #f8f4eb;
}
#mySetting-page .bar-nav{
    background: transparent;
    border: 0;
}
#mySetting-page .content{
	top: 0;
}
#mySetting-page .content .list-block ul{
    border-top: 0px ;
    border-bottom: 0px;
    margin-bottom: 4rem;
}
/*我的帮助*/
#myHelp-page{
	background-color: #0c265f;
}
#myHelp-page .bar-nav{
    background: #FFFFFF;
    border: 0;
}
#myHelp-page .content{
	top: 0;
}
#myHelp-page .content .list-block{
    position: absolute;
    top: 38%;
    width: 100%;
}
#myHelp-page .setBottomDiv{
    margin-top: 6rem;
}
#myHelp-page .list-block ul{
    width: 60%;
    margin: auto;
    background: transparent;
    border-top: 0px solid #e7e7e7;
    border-bottom: 1px solid #354a77;
    color: #ffffff;
}
#myHelp-page .list-block .item-inner{
    border-bottom: 1px solid #354a77;
    min-height: 2.2rem;
}
#myHelp-page .list-block .ul-title{
    color: #ffffff;
    width: 55%;
    margin: auto;
    font-size: 1.5rem;
    margin-bottom: .7rem;
    margin-top: .7rem;
}
#myHelp-page .list-block .icon-liBG{
    font-size: .8rem;
}
#myHelp-page .list-block .ul-title:before{
    content: "";
    background: url(../img/icon-ulBG.png) 0 center no-repeat;
    background-size: contain;
    height: 1.5rem;
    width: 1.5rem;
    display: inline-block;
    margin-right: .5rem;
}
/*个人资料*/
#myInfo-page{
	background: #F3F3F3;
}
#myinfo-page .bar-nav{
	background-color: #FAFAFA;
}
#myInfo-page .list-block{
    margin: 0;
    margin-bottom: 1rem;
}
#myInfo-page .list-block ul{
    border-top: 0px ;
    border-bottom: 0px;
}
#myInfo-page .head-li{
    padding: .5rem 0;
}
#myInfo-page .head-li .icon-head{
    position: relative;
    padding-left: 1.8rem;
    padding-top: .4rem;
}
#myInfo-page .head-li .icon-head:before{
    content: '';
    background: url(../img/icon-head.png) center no-repeat;
    background-size: contain;
    width: 1.5rem;
    height: 1.5rem;
    display: inline-block;
    position: absolute;
    left: 0;
    top: .2rem;
}
#myInfo-page .head-li .headPic,
.wrap .headPic{
    height: 3rem;
    width: 3rem;
    border-radius: 50%;
}
#myInfo-page .ul-marginLeft .item-inner {
    padding-left: 0;
    margin-left: .5rem;
}
.wrap .bottomBtn {
    position: fixed;
    width: 100%;
    bottom: 0;
}
#file{
    filter: alpha(opacity=0);
    opacity: 0;
    height: 4rem;
    position: absolute;
}
/*我的积分*/
#myPoint-page .bar-nav{
    background: transparent;
    border: 0;
}
#myPoint-page .content{
	top: 0;
	background-color: #FAFAFA;
}
#myPoint-page .bar-nav .ruleBtn{
	font-size: .8rem;
}
#myPoint-page .content .list{
	text-align: center;
	position: relative;
    height: 86%;
}
#myPoint-page .content .head-ul{
    margin: auto;
    background-color: #1d313a;
    border-radius: 50%;
    width: 8rem;
    height: 8rem;
    border: .7rem solid #ffffff;
    margin-top: -4rem;
    position: relative;
    -webkit-box-shadow: 0 0 1rem  rgba(196, 196, 196, 1);
    -moz-box-shadow: 0 0 1rem  rgba(196, 196, 196, 1);
    box-shadow: 0 0 1rem  rgba(196, 196, 196, 1);
}

#myPoint-page .head-ul .s-pointVal{
    margin-top: 30%;
    color: #ffffff;
    font-size: 1.5rem;
    font-weight: bold;
}
#myPoint-page .head-ul .s-pointTxt{
    color: #ffffff;
    font-size: .7rem;
}
#myPoint-page .list ul{
	padding: 0;
	margin: 0;
}
#myPoint-page .list .content-block{
    top: 8rem;
	padding: 0;
	margin: 0;
}
#myPoint-page .list .li-item{
    width: 50%;
    position: relative;
    height: 3.6rem;
}
#myPoint-page .list .li-item:before{
    content: '';
    display: inline-block;
    width: 1px;
    height: 3rem;
    background-color: #e5e6e0;
    position: absolute;
}
#myPoint-page .list .li-item:after{
    content: '';
    display: inline-block;
    width: .6rem;
    height: .6rem;
    background-color: #e5e6e0;
    border-radius: 50%;
    position: absolute;
    top: 3rem;
}
#myPoint-page .list .item-inner{
    position: absolute;
    top: 2rem;
    width: 100%;
}
#myPoint-page .list .div-txt{
    position: absolute;
    top: 0;
}
#myPoint-page .list .div-radius{
    display: inline-block;
    width: 2.2rem;
    height: 2.2rem;
    line-height: 2.2rem;
}
#myPoint-page .list .radius-green{
    color: #32a613;
    border-radius: 50%;
    border: .1rem solid #32a613;
    text-align: center;
    font-size: .5rem;
}
#myPoint-page .list .radius-red{
    color: #fd5b59;
    border-radius: 50%;
    border: .1rem solid #fd5b59;
    text-align: center;
    font-size: .5rem;
}
#myPoint-page .list .li-right{
    margin-left: 50%;
}
#myPoint-page .list .li-right:before{
    left: 0;
}
#myPoint-page .list .li-right:after{
    left: -.3rem;
}
#myPoint-page .list .li-right .item-inner{
    text-align: left;
    padding-left: .5rem;
}
#myPoint-page .list .li-right .div-txt{
    left: 2.8rem;
}
#myPoint-page .list .li-left:before{
    right: 0;
}
#myPoint-page .list .li-left:after{
    right: -.3rem;
}
#myPoint-page .list .li-left .item-inner{
    text-align: right;
    padding-right: .5rem;
}
#myPoint-page .list .li-left .div-txt{
    right: 2.8rem;
}
/*我的积分*/
#pointRule-page .content-block{
	background-color: #FFFFFF;
    margin: .5rem 0;
}
/*关于我们*/
#aboutUs-page .content-block{
	background-color: #FFFFFF;
    margin: .5rem 0;
}
/*我的消息*/
#myNews-page{
	background: #F3F3F3;
}
#myNews-page .bar-nav{
	background-color: #FAFAFA;
}
#myNews-page .content{
    top: 2.2rem;
}
#myNews-page .list-block{
    margin: 0;
}
#myNews-page .list-block ul{
	background-color: transparent;
}
#myNews-page .list-block ul li{
	margin-top: .5rem;
}
#myNews-page .card{
    box-shadow: none;
    margin: 0;
}
#myNews-page .list-block ul,
#myNews-page .card-footer{
	border: 0;
}
#myNews-page .card-title{
    font-size: 1rem;
    font-weight: bold;
    color: #000000;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}
#myNews-page .card-content-inner{
	padding-top: 0;
}

/*意见反馈*/
#feedback-page{
	background: #F8F4EB;
}
#feedback-page .bar-nav{
	background-color: #FAFAFA;
}
#feedback-page .setDiv{
	margin-top: .7rem;
	position: relative;
}
#feedback-page .list-block{
    position: absolute;
    width: 100%;
    top: 0;
    width: 74%;
    margin-left: 13%;
}
#feedback-page .list-block ul,
#feedback-page .list-block .item-inner{    
	border: 0;
    padding: 0;
}
#feedback-page .list-block .item-inner{    
	border: 1px solid #8f989D;
	border-radius: 4px;
}
#feedback-page .list-block .input-title{  
    margin: .5rem 0;
}
#feedback-page .list-block .icon-trigon:after{ 
    content: '';
    border-left: .5rem solid transparent;
    border-right: .5rem solid transparent;
    border-top: .5rem solid #1d313b;
    margin-right: .5rem;
    position: absolute;
    top: .7rem;
    right: 0;
    z-index: 1;
}
#feedback-page .j-feedbackBtn{
	font-size: .8rem;
}
#feedback-page .list-block select{ 
	position: relative;
	z-index: 2;
}
#feedback-page .list-block textarea{
	height: 9rem;
}

