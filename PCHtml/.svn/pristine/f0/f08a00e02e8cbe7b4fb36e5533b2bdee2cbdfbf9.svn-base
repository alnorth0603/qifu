/*首页关于我们*/

.aboutUs {
	display: block;
	position: absolute;
	top: 8%;
	z-index: 100;
	cursor: pointer;
	transition: opacity .9s cubic-bezier(.19, 1, .22, 1);
	position: absolute;
	top: 20px;
	right: 0;
}

.menbers {
	right: 170px;
	position: absolute;
	top: 20px;
	z-index: 100;
}

.menbers a {
	color: #fff;
	font-size: 16px;
	z-index: 100;
}

.aboutUs span {
	display: inline-block;
	position: relative;
	color: #fff;
	z-index: 2;
	transition: color .8s cubic-bezier(.19, 1, .22, 1), -webkit-transform .8s cubic-bezier(.77, 0, .175, 1);
	transition: color .8s cubic-bezier(.19, 1, .22, 1), transform .8s cubic-bezier(.77, 0, .175, 1);
	-webkit-transform: translate3d(0, 0, 0);
	transform: translate3d(0, 0, 0)
}

.aboutUs span:first-child {
	position: absolute;
	left: 0;
	top: -100%
}

.aboutUs .overflow-hidden {
	display: block;
	overflow: hidden;
	position: relative;
	transition: -webkit-transform .6s cubic-bezier(.19, 1, .22, 1);
	transition: transform .6s cubic-bezier(.19, 1, .22, 1)
}

